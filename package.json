{
  "name": "@test-ui/mocha",
  "version": "0.0.0-development",
  "description": "Mocha support for test-ui",
  "main": "dist/commonjs/src/index.js",
  "module": "dist/modules/src/index.js",
  "types": "dist/types/src/index.d.ts",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.north)",
  "ember-addon": {
    "main": "ember-addon.js"
  },
  "keywords": [
    "ember-addon"
  ],
  "scripts": {
    "problems": "node ./scripts/problems.js",
    "preversion": "npm run test",
    "prepare": "ember build -prod",
    "build": "ember build -prod",
    "build-tests": "ember build",
    "pretest": "npm run problems",
    "test": "ember test",
    "tdd": "ember test --server",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/travis-cli": "7.2.1",
    "@mike-north/js-lib-renovate-config": "1.1.5",
    "@mike-north/js-lib-semantic-release-config": "1.0.1",
    "@types/mocha": "5.2.5",
    "@types/qunit": "2.5.3",
    "ember-cli": "3.6.0",
    "husky": "1.2.1",
    "libkit": "mike-north/libkit#ts-3",
    "qunit": "2.9.0",
    "semantic-release": "15.12.4",
    "shelljs": "0.8.3",
    "travis-deploy-once": "5.0.10",
    "tslint": "5.12.0",
    "typescript": "3.2.2"
  },
  "dependencies": {
    "@test-ui/core": "^1.2.7",
    "mocha": "^5.2.0"
  },
  "plublishConfig": {
    "access": "public",
    "tag": "next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-north/test-ui-mocha.git"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
